<header>
  <nav  class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div style="text-align: right; clear: left; justify-content: end;" class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div style="text-align: right; clear: left;"class="navbar-nav">
        <a font ="system-ui" class="nav-item nav-link" href="/urls">My URLs</a>
        <a font="system-ui" class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <% if (!user) { %>
          <br>
          <div class="nav-bar-nav">
           <a class="btn btn-primary" font="16px" padding="8px" style="color:#FFFFFF80" href="/login">Login</a>
           <a class="btn btn-outline-primary" font="16px" padding="8px" style="color:#FFFFFF80" href="/register">Register</a>
          </div> 
        <% } %>
        <% if (user) { %>
          <form class="form-inline my-1" method="POST" action="/logout">
            <div>
              Logged in as: <%= user.email %>
               <button type="submit" class="btn btn-outline-dark">Logout</button>
            </div> 
          </form>
          <% } %>
      </div>
    </div>
  </nav>
</header>